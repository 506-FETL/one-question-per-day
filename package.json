{
  "name": "@one-question-per-day/workspace",
  "type": "module",
  "version": "2.2.0",
  "private": true,
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "author": "seaming",
  "scripts": {
    "dev": "run-s lint:fix lint test:run",
    "test": "vitest --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "pv": "pnpm --filter @one-question-per-day/problem-vue dev",
    "pr": "pnpm --filter @one-question-per-day/problem-react dev",
    "build:react": "pnpm --filter @one-question-per-day/problem-react build",
    "build:vue": "pnpm --filter @one-question-per-day/problem-vue build",
    "build:problems": "vite build && (cd dist && zip -rq -X problems.zip problems && rm -rf problems)",
    "lint": "eslint --config eslint.config.ts",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "release": "dotenv -f .env run -- release-it",
    "docs:dev": "pnpm --filter @one-question-per-day/docs dev",
    "docs:build": "pnpm --filter @one-question-per-day/docs build",
    "docs:preview": "pnpm --filter @one-question-per-day/docs preview"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@one-question-per-day/docs": "workspace:*",
    "@one-question-per-day/problem-react": "workspace:*",
    "@one-question-per-day/problem-vue": "workspace:*",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.12",
    "@types/mdx": "^2.0.13",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "lucide-vue-next": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "6.30.1",
    "react-router-dom": "6.30.1",
    "rehype-starry-night": "^2.2.0",
    "reka-ui": "^2.5.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "vitepress-plugin-group-icons": "^1.6.3",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@eslint/js": "^9.33.0",
    "@iconify-json/radix-icons": "^1.2.4",
    "@iconify/vue": "^5.0.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "archiver": "^7.0.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "markdown-it-prism": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prismjs": "^1.30.0",
    "release-it": "^19.0.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-static-copy": "^3.1.2",
    "vitepress": "2.0.0-alpha.12",
    "vitest": "^3.2.4"
  }
}
