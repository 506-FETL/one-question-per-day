  "scripts": {
    "dev:react": "cd react && pnpm dev",
    "dev:vue": "cd vue && pnpm dev",
    "test:run": "run-s test:run-base test:run-react test:run-vue",
    "test:vue": "cd vue && pnpm test",
    "test:react": "cd react && pnpm test",
    "test:base": "cd base && pnpm test",
    "test:run-vue": "cd vue && pnpm test:run",
    "test:run-react": "cd react && pnpm test:run",
    "test:run-base": "cd base && pnpm test:run",
    "lint": "run-p lint:base lint:react lint:vue",
    "lint:base": "cd base && pnpm lint",
    "lint:react": "cd react && pnpm lint",
    "lint:vue": "cd vue && pnpm lint",
    "lint:fix": "run-p lint:base:fix lint:react:fix lint:vue:fix",
    "lint:base:fix": "cd base && pnpm lint:fix",
    "lint:react:fix": "cd react && pnpm lint:fix",
    "lint:vue:fix": "cd vue && pnpm lint:fix"
  }
